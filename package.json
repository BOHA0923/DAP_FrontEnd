{
  "name": "ng-webdpt",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "postinstall": "ngcc",
    "ng": "ng",
    "start": "node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng serve",
    "start-prod": "ng serve --prod",
    "build": "ng build",
    "test": "ng test",
    "showcase-test": "ng test showcase-app --code-coverage --watch",
    "framework-test": "ng test @webdpt/framework --code-coverage --watch",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "buildCss": "node themeCssBuild",
    "generate-i18n-files": "ngx-translate-extract -i ./src -o ./src/i18n/default-en.json --clean --sort --format namespaced-json",
    "build-prod": "node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng build --prod",
    "doc:build": "compodoc -p src/tsconfig.app.json -d ./doc",
    "doc:serve": "compodoc -s -r 4201 -d ./doc",
    "doc:buildandserve": "compodoc -p ./tsconfig.json -s -r 4201 -d ./doc",
    "framework:packagr1": "node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng build @webdpt/framework",
    "framework:packagr2": "node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng build @webdpt/components",
    "framework:packagr3": "node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng build @webdpt/programs",
    "framework:packagr4": "node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng build @webdpt/analytics",
    "framework:packagr": "npm run framework:packagr1 && npm run framework:packagr2 && npm run framework:packagr3 && npm run framework:packagr4",
    "framework:less": "cpx \"projects/webdpt/framework/**/*.less\" \"dist/webdpt/framework\" --verbose",
    "framework:assets1": "cpx \"projects/webdpt/framework/assets/exception/**/*\" \"dist/webdpt/framework/assets/exception\" --verbose",
    "framework:assets2": "cpx \"projects/webdpt/framework/assets/dw-framework/**/*\" \"dist/webdpt/framework/assets/dw-framework\" --verbose",
    "framework:assets3": "cpx \"projects/webdpt/components/style/**/*\" \"dist/webdpt/components/style\" --verbose",
    "framework:assets": "npm run framework:assets1 && npm run framework:assets2 && npm run framework:assets3",
    "framework:build": "npm run framework:packagr && npm run framework:less && npm run framework:assets",
    "showcase": "ng serve showcase-app --port=4220 --open",
    "showcase-build-prod": "node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng build showcase-app --prod",
    "sonar": "sonar-scanner",
    "gmc": "node projects/webdpt/builds/generate.js",
    "gmc-test": "node projects/webdpt/builds/generate.js --name server-doc --folder src/app/implementation --route-name server-doc --parent-module implementation.module.ts --routing --components server-doc-list,server-doc-modify"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/schematics": "^9.1.8",
    "@angular/animations": "^9.0.0",
    "@angular/cdk": "^9.0.0",
    "@angular/common": "^9.0.0",
    "@angular/compiler": "^9.1.11",
    "@angular/core": "^9.0.0",
    "@angular/forms": "^9.0.0",
    "@angular/platform-browser": "^9.0.0",
    "@angular/platform-browser-dynamic": "^9.1.11",
    "@angular/pwa": "0.901.11",
    "@angular/router": "^9.0.0",
    "@angular/service-worker": "^9.1.11",
    "@ant-design/icons-angular": "^9.0.0",
    "@fullcalendar/angular": "4.4.5-beta",
    "@fullcalendar/core": "4.4.2",
    "@fullcalendar/daygrid": "4.4.2",
    "@fullcalendar/interaction": "4.4.2",
    "@fullcalendar/list": "4.4.2",
    "@fullcalendar/rrule": "4.4.2",
    "@fullcalendar/timegrid": "4.4.2",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@types/requirejs": "^2.1.31",
    "ag-grid-angular": "^19.0.0",
    "ag-grid-community": "^19.0.0",
    "angular-gridster2": "9.2.0",
    "angular-in-memory-web-api": "^0.9.0",
    "classlist.js": "1.1.20150312",
    "core-js": "^2.4.1",
    "crypto-js": "^3.1.9-1",
    "date-fns": "^2.10.0",
    "fast-deep-equal": "^2.0.1",
    "jsencrypt": "^3.0.0-rc.1",
    "moment": "2.24.0",
    "ng-quicksilver": "file:projects/ng-quicksilver.tar.gz",
    "ngx-color": "5.1.3",
    "ngx-highlightjs": "^2.1.1",
    "resize-observer-polyfill": "^1.5.1",
    "rrule": "2.6.0",
    "rxjs": "~6.5.3",
    "sonarqube-scanner": "^2.4.1",
    "tslib": "^1.10.0",
    "tslint-sonarts": "^1.9.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.8",
    "@angular-devkit/build-ng-packagr": "~0.901.8",
    "@angular-devkit/core": "^9.1.8",
    "@angular/cli": "^9.1.8",
    "@angular/compiler-cli": "^9.1.11",
    "@angular/language-service": "^9.1.11",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@compodoc/compodoc": "^1.1.3",
    "@phenomnomnominal/angular-lazy-routes-fix": "^0.1.5",
    "@phenomnomnominal/tsquery": "^4.1.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "commander": "^5.1.0",
    "cpx": "^1.5.0",
    "inquirer": "^7.3.0",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "less": "^3.10.3",
    "ng-packagr": "^9.0.0",
    "node-sass": "^4.13.1",
    "protractor": "~5.4.0",
    "raw-loader": "0.5.1",
    "ts-node": "~7.0.0",
    "tslint": "^5.7.0",
    "typescript": "~3.8.3",
    "webpack-bundle-analyzer": "^3.7.0"
  }
}
